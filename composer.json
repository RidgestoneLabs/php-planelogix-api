{
  "name": "ridgestonelabs/planelogix-api-php",
  "homepage": "https://github.com/RidgestoneLabs/planelogix-api-php",
  "description": "PLANELOGIX API v2 client for PHP",
  "keywords": [
    "planelogix",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew Kolstad",
      "email": "hello@andrewkolstad.com",
      "homepage": "https://github.com/DrewKolstad"
    }
  ],
  "require": {
    "php": "^7.4.15 || ^8.0.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.5",
    "http-interop/http-factory-guzzle": "^1.0",
    "php-http/client-common": "^2.5",
    "php-http/discovery": "^1.14",
    "php-http/httplug": "^2.2",
    "psr/http-client-implementation": "^1.0",
    "psr/http-factory-implementation": "^1.0",
    "psr/http-message": "^1.0",
    "symfony/polyfill-php80": "^1.17"
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.4.1"
  },
  "autoload": {
    "psr-4": {
      "PlaneLogixAPI\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PlaneLogixAPI\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "allow-plugins": {
      "bamarni/composer-bin-plugin": true
    }
  }
}
